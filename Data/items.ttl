    dcterms:dateSubmitted "2022-06-28" ;
    dcterms:description "Jurk van witte katoen met blauwe en roze ruiten." ;
    dcterms:extent "hoogte: 100 cm  schouderbreedte: 36 cm  tailleomvang: 62 cm" ;
    dcterms:identifier <http://data.momu.be/item/id/mjwt0r> ;
